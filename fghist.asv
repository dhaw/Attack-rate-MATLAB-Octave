function f=fghist(f,g)
l=length(f);
burn=10;
F=f(burn+1:end); G=g(burn+1:end);
maxz
fs=15;
figure
subplot(2,1,1)
hist(F,(0:int:maxz));
maxF=max(hist(F,(0:int:maxz));
axis([0,1,0,maxF]);
xlabel('Proportion immune','FontSize',fs)
ylabel('Frequency','FontSize',fs)
subplot(2,1,2)
hist(G,(0:int:maxz));
axis([0,1,0,maxF]);
xlabel('Proportion immune','FontSize',fs)
ylabel('Frequency','FontSize',fs)